<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">

<hibernate-reverse-engineering>
	<schema-selection match-catalog="sysprod"
		match-schema="dbo" />
	<table-filter match-name="INFO_DOCUMENTO" />
	<table-filter match-name="DESCUENTO_CALCIOS_TABLA_A" />
	<table-filter match-name="DESCUENTO_HUMEDAD_TABLA_B" />
	<table-filter match-name="DESCUENTO_HUMEDAD_TABLA_A" />
	<table-filter match-name="TRANSPORTISTA" />
	<table-filter match-name="FOLIOS" />
	<table-filter match-name="PERFILES" />
	<table-filter match-name="PRESERVACION" />
	<table-filter match-name="MATERIA" />
	<table-filter match-name="PROVEEDORES" />
	<table-filter match-name="PRECIOS" />
	<table-filter match-name="DESCUENTO_CALCIOS_TABLA_B" />
	<table-filter match-name="ENTRADAS" />
	<table-filter match-name="USUARIOS" />
</hibernate-reverse-engineering>
